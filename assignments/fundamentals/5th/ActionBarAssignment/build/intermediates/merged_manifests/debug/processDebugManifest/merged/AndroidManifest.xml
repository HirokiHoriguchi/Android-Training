<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.assignment.actionbar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="jp.mixi.assignment.actionbar.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mixi.assignment.actionbar.Actionbar1Activity"
            android:label="@string/actionbar1_name" />
        <activity
            android:name="jp.mixi.assignment.actionbar.Actionbar2Activity"
            android:label="@string/actionbar2_name" />
        <activity
            android:name="jp.mixi.assignment.actionbar.Interaction1Activity"
            android:label="@string/interaction1_name" />
        <activity
            android:name="jp.mixi.assignment.actionbar.Interaction2Activity"
            android:label="@string/interaction2_name" />
    </application>

</manifest>
<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.practice.layout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="jp.mixi.practice.layout.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mixi.practice.layout.LinearLayout1Activity"
            android:label="@string/linear_layout1_name" />
        <activity
            android:name="jp.mixi.practice.layout.LinearLayout2Activity"
            android:label="@string/linear_layout2_name" />
        <activity
            android:name="jp.mixi.practice.layout.RelativeLayout1Activity"
            android:label="@string/relative_layout1_name" />
        <activity
            android:name="jp.mixi.practice.layout.RelativeLayout2Activity"
            android:label="@string/relative_layout2_name" />
        <activity
            android:name="jp.mixi.practice.layout.FrameLayout1Activity"
            android:label="@string/frame_layout1_name" />
        <activity
            android:name="jp.mixi.practice.layout.FrameLayout2Activity"
            android:label="@string/frame_layout2_name" />
        <activity
            android:name="jp.mixi.practice.layout.ScrollViewActivity"
            android:label="@string/scroll_view1_name" />
    </application>

</manifest>
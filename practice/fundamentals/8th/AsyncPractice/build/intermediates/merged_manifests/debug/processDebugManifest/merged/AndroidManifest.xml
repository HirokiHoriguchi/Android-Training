<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.mixi.practice.async"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="jp.mixi.practice.async.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jp.mixi.practice.async.ServicesActivity"
            android:label="@string/services_name" />
        <activity
            android:name="jp.mixi.practice.async.AsyncTaskLoaderActivity"
            android:label="@string/async_task_loader_name" />
        <activity
            android:name="jp.mixi.practice.async.AsyncTaskActivity"
            android:label="@string/async_task_name" />

        <service android:name="jp.mixi.practice.async.BoundService" />
        <service android:name="jp.mixi.practice.async.MyIntentService" />
        <service android:name="jp.mixi.practice.async.StartedService" />
    </application>

</manifest>